{
    "name": "rice-discuss-backend",
    "version": "1.1.0",
    "description": "The backend for RiceDiscuss, written in Node and Apollo GraphQL",
    "main": "src/index.js",
    "scripts": {
        "prod": "run-s build serve",
        "lint": "eslint --fix .",
        "build": "rm -rf build && babel src -d build",
        "start": "nodemon",
        "serve": "node --trace-warnings build/index.js",
        "format": "prettier --write .",
        "make-exec": "pkg build/index.js -o build/RiceDiscussBackend --options trace-warnings",
        "pkg": "run-s build make-exec"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rice-apps/RiceDiscuss-backend.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rice-apps/RiceDiscuss-backend/issues"
    },
    "homepage": "https://github.com/rice-apps/RiceDiscuss-backend#readme",
    "devDependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.11.0",
        "@babel/node": "^7.8.7",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@types/babel-core": "^6.25.6",
        "@types/express": "^4.17.6",
        "@types/mongoose": "^5.7.34",
        "@types/node": "^14.0.27",
        "@types/ws": "^7.2.4",
        "eslint": "^7.6.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "pkg": "^4.4.9",
        "prettier": "^2.0.5"
    },
    "dependencies": {
        "@babel/runtime": "^7.11.0",
        "apollo-server-express": "^2.16.1",
        "aws-sdk": "^2.725.0",
        "bent": "^7.3.9",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-ws": "^4.0.0",
        "graphql": "^14.6.0",
        "graphql-compose": "^7.15.0",
        "graphql-compose-connection": "^6.2.1",
        "graphql-compose-mongoose": "^7.3.8",
        "jsonwebtoken": "^8.5.1",
        "loglevel": "^1.6.8",
        "mongoose": "^5.9.27",
