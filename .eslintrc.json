{
    "env": {
        "browser": false,
        "es6": true
    },
    "extends": ["google", "prettier", "plugin:prettier/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "require-jsdoc": ["off"]
    }
}
